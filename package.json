{
  "name": "fileglancer",
  "version": "0.1.0",
  "description": "Browse, share, and publish files on the Janelia file system",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/JaneliaSciComp/fileglancer",
  "bugs": {
    "url": "https://github.com/JaneliaSciComp/fileglancer/issues"
  },
  "license": "BSD-3-Clause",
  "author": "allison-truhlar",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "src/**/*.{ts,tsx}"
  ],
  "main": "lib/src/index.js",
  "types": "lib/src/index.d.ts",
  "style": "style/index.css",
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.extension.json && tsc -b && vite build && npm run build:labextension:dev",
    "build:prod": "tsc -p tsconfig.extension.json &&tsc -b && vite build --mode production && npm run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "watch": "run-p watch:src watch:labextension",
    "watch:src": "tsc -b --watch && vite build --watch",
    "watch:labextension": "jupyter labextension watch .",
    "lint": "eslint .",
    "install:extension": "npm run build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@jupyterlab/application": "^4.0.0",
    "@jupyterlab/apputils": "^4.4.4",
    "@jupyterlab/coreutils": "^6.0.0",
    "@jupyterlab/launcher": "^4.3.4",
    "@jupyterlab/services": "^7.0.0",
    "@jupyterlab/ui-components": "^4.3.4",
    "@material-tailwind/react": "^3.0.0-beta.24",
    "autoprefixer": "^10.4.21",
    "iconoir-react": "^7.10.1",
    "npm-run-all2": "^7.0.2",
    "postcss": "^8.5.3",
    "react": "18.3",
    "react-cookie": "^8.0.1",
    "react-dom": "18.3",
    "react-router": "^7.4.1",
    "react-router-dom": "^7.4.1",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@jupyterlab/builder": "^4.0.0",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "prettier": "^3.5.3",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "discovery": {
      "server": {
        "managers": [
          "pip"
        ],
        "base": {
          "name": "fileglancer"
        }
      }
    },
    "extension": true,
    "outputDir": "fileglancer/labextension"
  }
}
